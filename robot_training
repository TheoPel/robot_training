cmds.setAttr(mouth[0] + '.scaleX' , 1.036)
cmds.setAttr(mouth[0] + '.scaleY' , 0.535)
cmds.setAttr(mouth[0] + '.scaleZ' , 0.511)

eyes = cmds.polyTorus( ch=False, name = 'eyes')
