import maya.cmds as cmds

#Head

head = cmds.polySphere( ch=False, name = 'head')
cmds.setAttr(head[0] + '.translateZ' , 23.305)
cmds.setAttr(head[0] + '.scaleX' , 3.909)
cmds.setAttr(head[0] + '.scaleY' , 2.331)
cmds.setAttr(head[0] + '.scaleZ' , 4.283)

mouth = cmds.polyCylinder( ch=False, name = 'mouth')
